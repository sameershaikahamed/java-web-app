pipeline {
  agent any
tools {
        maven 'maven' // Name must match the Maven installation in Jenkins Global Tool Configuration
    }
 
  stages {
    stage('Scan') {
      steps {
        withSonarQubeEnv(installationName: 'SonarQube_Server') { 
          sh 'mvn sonar:sonar'
        }
      }
    }
  }
}
